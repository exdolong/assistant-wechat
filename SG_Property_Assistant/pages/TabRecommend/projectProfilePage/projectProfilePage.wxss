/* pages/TabRecommend/projectProfilePage/projectProfilePage.wxss */

page {
  background: #f5f5f5;
}

.building_introd {
  background: #fff;
  padding: 30rpx;
  margin-bottom: 20rpx;
}

.title_wenben {
  font-size: 34rpx;
  font-weight: 500;
  display: block;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}

.content_zhong {
  font-size: 30rpx;
  color: #333;
  display: block;
  line-height: 60rpx;
}

.map {
  height: 380rpx;
  background: #fff;
  width: 100%;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}

.bitmap {
  width: 100%;
  height: 410rpx;
  margin-top: 40rpx;
}

.facilities_box {
  background: #fff;
  padding: 30rpx;
  padding-top: 6rpx;
}

.title_biaoti {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-bottom: 40rpx;
  margin-top: 40rpx;
}

.icon_mi {
  width: 44rpx;
  height: 44rpx;
  float: left;
  margin-right: 16rpx;
}

.text_bt {
  font-size: 34rpx;
  color: #333;
  font-weight: 500;
  line-height: 44rpx;
}

.north_line text {
  font-size: 30rpx;
  color: #666;
  display: block;
}

.north_line {
  margin-top: 20rpx;
}

.btn{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  background: #fff;
  justify-content: space-between;
  padding-left: 60rpx;
  padding-right: 60rpx;
  height: 80rpx;
  width: calc(100% - 120rpx);
  position: fixed;
  top: 0px;
  z-index: 9009;
  border-bottom: 1px solid #eee;
  box-shadow: 0px 2px 5px #eee;
}

.btn text {
  font-size: 34rpx;
  font-weight: 500;
  line-height: 80rpx;
  color: #999;
}

.btn text.on{
  color: #333
}

.box{height:100%;}